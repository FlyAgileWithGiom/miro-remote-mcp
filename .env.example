# Miro OAuth2 Credentials
# Get these from: https://miro.com/app/settings/user-profile/apps
MIRO_CLIENT_ID=your_client_id_here
MIRO_CLIENT_SECRET=your_client_secret_here

# OAuth2 Tokens (will be managed automatically by oauth-helper.ts)
MIRO_ACCESS_TOKEN=
MIRO_REFRESH_TOKEN=

# OAuth2 Configuration
MIRO_REDIRECT_URI=http://localhost:3003/oauth/callback

# Server Configuration
PORT=3003
NODE_ENV=development

# Optional: Test Board ID for debug scripts
MIRO_TEST_BOARD_ID=
